    "klaytn",
    "nft",
    "defi",
    "web3",
    "signing",
    "cold-storage"
  ],
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "files": [
    "dist",
    "LICENSE",
    "COMMERCIAL_LICENSE.md",
    "LICENSING_FAQ.md",
    "README.md"
  ],
  "n8n": {
    "n8nNodesApiVersion": 1,
    "credentials": [
      "dist/credentials/DCentDevice.credentials.js",
      "dist/credentials/DCentBridge.credentials.js",
      "dist/credentials/DCentNetwork.credentials.js"
    ],
    "nodes": [
      "dist/nodes/DCent/DCent.node.js",
      "dist/nodes/DCent/DCentTrigger.node.js"
    ]
  },
  "scripts": {
    "build": "tsc && gulp build:icons",
    "dev": "tsc --watch",
    "lint": "eslint . --ext .ts",
    "lint:fix": "eslint . --ext .ts --fix",
    "format": "prettier --write \"**/*.ts\"",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
