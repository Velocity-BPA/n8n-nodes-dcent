  },
  "homepage": "https://github.com/Velocity-BPA/n8n-nodes-dcent#readme",
  "bugs": {
    "url": "https://github.com/Velocity-BPA/n8n-nodes-dcent/issues"
  },
  "keywords": [
    "n8n",
    "n8n-community-node-package",
    "dcent",
    "d'cent",
    "hardware-wallet",
    "cryptocurrency",
    "bitcoin",
    "ethereum",
    "blockchain",
    "biometric",
    "multi-chain",
