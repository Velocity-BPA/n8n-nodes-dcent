  "engines": {
    "node": ">=18.0.0"
  },
  "dependencies": {
    "ethers": "^6.9.0",
    "bitcoinjs-lib": "^6.1.5",
    "@solana/web3.js": "^1.87.6",
    "node-hid": "^3.0.0",
    "@noble/hashes": "^1.3.3",
    "uuid": "^9.0.0"
